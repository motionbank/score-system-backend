<style type="text/css">@import url('/stylesheets/sets.css');</style>

<h1>You have <%= user.sets.length %> sets</h1>
<br/>
<div class="list set-list">
<% for ( var i = 0; i < user.sets.length; i++ ) { %>
	<div class="list-item">
		<div class="poster">
			<img src="http://<%= config.media.host %><%= config.media.basePath %>/sets/poster/small/<%= user.sets[i].poster %>" />
		</div>
		<div class="title">
			<a href="/admin/sets/<%= user.sets[i].id %>/layout"><%= user.sets[i].title %></a>
		</div>
	</div>
<% } %>
</div>