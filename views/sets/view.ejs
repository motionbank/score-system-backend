<% include ../_header %>
	<section id="main">
		<style type="text/css">
			
		</style>
		<h1 class="title"><%= set.title %></h1>
		<br />
		<div class="actions">
			<a href="/admin/sets/<%= set.id %>/edit">Edit</a> <a href="/admin/sets/<%= set.id %>/layout">Layout</a> <a href="/admin/sets/<%= set.id %>/delete" confirm="Delete this set?">Delete</a>
		</div>
		<br />

		<div class="poster">
			<img src="http://<%= config.media.host %><%= config.media.basePath %>/sets/poster/full/<%= set.poster %>" style="max-height:300px" />
			<img src="http://<%= config.media.host %><%= config.media.basePath %>/sets/poster/medium/<%= set.poster %>" />
			<img src="http://<%= config.media.host %><%= config.media.basePath %>/sets/poster/small/<%= set.poster %>" />
		</div>
		<br/>

		<% include ../cells/_list %>

	</section>
<% include ../_footer %>